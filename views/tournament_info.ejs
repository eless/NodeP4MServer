<html lang="en" ng-app="app">
<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular.min.js"></script>
<script src="/js/index.js"></script>
<h2><%=description%></h2>
<body ng-controller="tournamentDataCtrl" ng-init="init('<%=tournament_id%>')">
<table st-table="playersInTournament" class="table table-striped">
    <thead>
        <tr>
            <th ng-repeat="col in tableHeaders">{{col}}</th>
        </tr>
    </thead>
    <tbody>
        <tr ng-repeat="line in playersInTournament">
            <td><a ng-href="/profile/{{line.account_id}}">{{line['steam_name']}}</a></td>
            <td  ng-repeat="column in tableHeaders" ng-if= "column != 'steam_name'">{{line[column]}}</td>
        </tr>
    </tbody>
    <tfoot>
    <tr>
        <td colspan="{{tableHeaders.length}}" class="text-center">
            <div st-pagination="" st-items-by-page="{{itemsByPage}}" st-displayed-pages="7"></div>
        </td>
    </tr>
    </tfoot>
</table>
</body>
</html>